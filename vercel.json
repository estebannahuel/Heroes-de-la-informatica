{
  "version": 2,
  "routes": [
    {
      "src": "/(.*\\.html)",
      "headers": { "Content-Type": "text/html" },
      "continue": true
    },
    {
      "src": "/(.*\\.css)",
      "headers": { "Content-Type": "text/css" },
      "continue": true
    },
    { "handle": "filesystem" },
    { "src": "/.*", "status": 404, "dest": "/404.html" }
  ]
}
